<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="720.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ModifyCustomerController">
    <children>
        <Label fx:id="ModifyCustomerLbl" layoutX="33.0" layoutY="69.0" text="Modify Customer">
        </Label>
        <GridPane fx:id="ModifyCustomerPane" layoutX="33.0" layoutY="133.0" prefHeight="417.0" prefWidth="523.0" vgap="20.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label fx:id="ModifyCustomerIDLbl" text="ID" />
                <Label fx:id="ModifyCustomerNameLbl" layoutX="10.0" layoutY="28.0" text="Name" GridPane.rowIndex="1" />
                <Label fx:id="ModifyCustomerAddressLbl" layoutX="10.0" layoutY="80.0" text="Address" GridPane.rowIndex="2" />
                <Label fx:id="ModifyCustomerPhoneLbl" text="Phone Number" GridPane.rowIndex="3" />
                <Label fx:id="ModifyCustomerCountryLbl" text="Country" GridPane.rowIndex="4" />
                <Label fx:id="ModifyCustomerStateLbl" text="State" GridPane.rowIndex="5" />
                <Label fx:id="ModifyCustomerCodeLbl" text="Postal Code" GridPane.rowIndex="6" />
                <TextField fx:id="ModifyCustomerIDTxt" editable="false" GridPane.columnIndex="1" />
                <TextField fx:id="ModifyCustomerNameTxt" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="ModifyCustomerAddressTxt" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="ModifyCustomerPhoneTxt" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <ComboBox fx:id="ModifyCustomerCountryCombo" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                <ComboBox fx:id="ModifyCustomerStateCombo" onAction="#OnActionCustomerState" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                <TextField fx:id="ModifyCustomerCodeTxt" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            </children>
        </GridPane>
        <Button fx:id="ModifyCustomerSaveBtn" layoutX="419.0" layoutY="596.0" mnemonicParsing="false" onAction="#OnActionSaveModifyCustomer" text="Save" />
        <Button fx:id="ModifyCustomerCancelBtn" layoutX="472.0" layoutY="596.0" mnemonicParsing="false" onAction="#OnActionCancelModifyCustomer" text="Cancel" />
    </children>
</AnchorPane>
